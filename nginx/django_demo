server {
    listen 80;
    server_name <서버 ip 주소>;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /home/ubuntu/sus_3/static/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/sus_3/gunicorn/django.sock;
    }
}